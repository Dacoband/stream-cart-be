{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/accounts/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "brightpa.me",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/api/accounts/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "SwaggerKey": "account"
    },
    {
      "DownstreamPathTemplate": "/api/auth/login",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "brightpa.me",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/api/auth/login",
      "UpstreamHttpMethod": [ "POST" ],
      "SwaggerKey": "account"
    },
    {
      "DownstreamPathTemplate": "/api/auth/register",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "brightpa.me",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/api/auth/register",
      "UpstreamHttpMethod": [ "POST" ],
      "SwaggerKey": "account"
    },
    {
      "DownstreamPathTemplate": "/api/auth/reset-password-request",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "brightpa.me",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/api/auth/reset-password-request",
      "UpstreamHttpMethod": [ "POST" ],
      "SwaggerKey": "account"
    },
    {
      "DownstreamPathTemplate": "/api/auth/verify",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "brightpa.me",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/api/auth/verify",
      "UpstreamHttpMethod": [ "GET" ],
      "SwaggerKey": "account"
    },
    {
      "DownstreamPathTemplate": "/api/auth/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "brightpa.me",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/api/auth/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "SwaggerKey": "account",
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer"
      }
    },
    {
      "DownstreamPathTemplate": "/swagger/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "brightpa.me",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/swagger/account/{everything}",
      "UpstreamHttpMethod": [ "GET" ]
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "account",
      "Config": [
        {
          "Name": "Account API",
          "Version": "v1",
          "Url": "https://brightpa.me/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://brightpa.me"
  }
}